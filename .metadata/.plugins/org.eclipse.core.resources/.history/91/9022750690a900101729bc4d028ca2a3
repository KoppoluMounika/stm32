#include "lcd.h"

int main(void)
{
    // Initialize GPIOs for LCD
    GPIO_PeriClockControl(GPIOB, ENABLE);
    GPIO_PeriClockControl(GPIOC, ENABLE);
    // Init pins using your GPIO driver
    // ... same as before

    LCD_Init();
    LCD_SendString("Hello STM32");
    LCD_SetCursor(1,0);
    LCD_SendString("1602A LCD");

    while(1);
}
